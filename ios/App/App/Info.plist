	<string>This app needs access to microphone for video calls with teachers</string>
	<key>NSLocalNetworkUsageDescription</key>
	<string>This app needs access to local network for video call functionality</string>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
	</dict>
